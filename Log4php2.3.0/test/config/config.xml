<?xml version="1.0" encoding="UTF-8"?>
<configuration xmlns="http://logging.apache.org/log4php/">
    <appender name="spkAppender" class="LoggerAppenderFile">
        <param name="file" value="spkLog.log" />
    </appender>
    <root>
        <level value="WARN" />
        <appender_ref ref="spkAppender" />
    </root>
</configuration>